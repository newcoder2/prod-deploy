        txtNAME.Activate
    End If

PDWflag = False 'SETTING FLAG IN ORDER TO INDICATE PDW LIST BUTTON HAS NOT BEEN USED

ERROR:
If ERR.Number Then
    MsgBox "cmdNameSEARCH buttom :: " & ERR.Description, vbCritical, "ENGAGEMENT PROCESS"
    ERR.Clear
End If
End Sub

